{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome against localhost, with sourcemaps",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8080",
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}"
        },
         {
            "name": "Ejemplo 10 Ferrari",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8080/mean_mio/mean/javascript/ejemplo02-htmljavascript/ejemplo10ferrarifinal.html",
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}"
        },
        {
            "name": "Attach to Chrome, with sourcemaps",
            "type": "chrome",
            "request": "attach",
            "port": 2226,
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}"
        }
    ]
}